# Prime Factorization of a number

def primefactor(n):
    x = int(n**0.5)+1
    for i in range(2,x):
        if n%i == 0:
            count = 0
            while n%i == 0:
                count +=1
                n = int(n/i)
            
            print([i,count])
        

n = 12
primefactor(n)
